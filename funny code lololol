#include <iostream>
using namespace std;

int main()
{
    //Input number
    int height,width,tall;
    int volume;
    
    cout<<"p = ";
    cin>>height;
    cout<<"\nl = ";
    cin>>width;
    cout<<"\nt = ";
    cin>>tall;
    cout<<"============================\n\n";
    
    //Operaso
    volume=height*width*tall;
    
    //Output volume
    cout<<"jika panjang "<<height<<", lebar "<<width<<", dan tinggi "<<tall<<",\nMaka volume balok adalah "<<volume;
    
    return 0;
}
